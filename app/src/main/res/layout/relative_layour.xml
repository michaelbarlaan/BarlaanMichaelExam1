<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <EditText
        android:id="@+id/editText"
        android:layout_width="126dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Lorem Ipsum"
        tools:text="Lorem Ipsum" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="47dp"
        android:layout_height="42dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="12dp"
        app:srcCompat="@android:drawable/ic_search_category_default" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@android:drawable/ic_menu_sort_by_size" />

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editText"
        android:text="Profile"
        android:textColorHint="?android:attr/textColorLinkInverse"
        android:textColorLink="@android:color/holo_red_light" />

    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="96dp"
        android:layout_above="@+id/button6"
        android:layout_alignParentStart="true"
        android:textColorHighlight="@android:color/holo_red_light"
        android:textColorHint="?android:attr/textColorLinkInverse"
        android:textColorLink="@android:color/holo_red_light" />

    <ImageButton
        android:id="@+id/previous"
        android:layout_width="93dp"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="59dp"
        android:cropToPadding="false"
        app:srcCompat="?android:attr/fastScrollPreviewBackgroundLeft" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="96dp"
        android:layout_height="83dp"
        android:layout_alignStart="@+id/previous"
        android:layout_alignTop="@+id/editText2"
        android:visibility="visible"
        app:srcCompat="@android:drawable/ic_menu_my_calendar" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/editText"
        android:layout_marginTop="146dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="John Doe"
        android:visibility="visible" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/editText"
        android:layout_marginTop="196dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Lorem Ipsum Dolor"
        android:visibility="visible" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="52dp"
        android:layout_height="48dp"
        android:layout_alignStart="@+id/editText"
        android:layout_alignTop="@+id/imageButton11"
        app:srcCompat="@android:drawable/sym_def_app_icon" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="56dp"
        android:layout_height="46dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="209dp"
        android:layout_toStartOf="@+id/button6"
        app:srcCompat="@drawable/moneydollarcircle512" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton9"
        android:layout_toEndOf="@+id/textView7"
        app:srcCompat="@android:drawable/btn_star_big_on" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageButton16"
        android:layout_alignTop="@+id/imageButton9"
        app:srcCompat="@android:drawable/ic_input_add" />

    <Button
        android:id="@+id/button6"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="124dp"
        android:text="Connect now" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="62dp"
        android:layout_height="53dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imageButton11"
        android:layout_marginBottom="59dp"
        app:srcCompat="@android:drawable/ic_menu_manage" />

    <ImageButton
        android:id="@+id/imageButton15"
        android:layout_width="66dp"
        android:layout_height="57dp"
        android:layout_alignTop="@+id/imageButton14"
        android:layout_centerHorizontal="true"
        app:srcCompat="@android:drawable/ic_lock_idle_alarm" />

    <ImageButton
        android:id="@+id/imageButton16"
        android:layout_width="66dp"
        android:layout_height="57dp"
        android:layout_alignBottom="@+id/imageButton15"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="35dp"
        app:srcCompat="@android:drawable/ic_menu_info_details" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="51dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageButton11"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="183dp"
        android:text="Money" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageButton9"
        android:layout_below="@+id/imageButton9"
        android:text="APP" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageButton12"
        android:layout_below="@+id/imageButton9"
        android:text="Favorites" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton9"
        android:layout_toEndOf="@+id/button6"
        android:text="Select" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageButton11"
        android:layout_alignTop="@+id/textView7"
        android:text="Settings" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="27dp"
        android:text="Alarm" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="36dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageButton13"
        android:layout_alignTop="@+id/textView6"
        android:text="Info" />

    <ImageButton
        android:id="@+id/imageButton17"
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:layout_alignEnd="@+id/imageButton3"
        android:layout_alignTop="@+id/previous"
        app:srcCompat="?android:attr/fastScrollPreviewBackgroundRight" />

</RelativeLayout>